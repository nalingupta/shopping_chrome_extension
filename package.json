{
    "name": "shopping-chrome-extension",
    "version": "0.18.0",
    "scripts": {
        "build": "echo 'No build process needed for this extension'",
        "lint": "echo 'Linting completed - no linter configured'",
        "server": "zsh scripts/server.sh --reload --log-level debug --restart",
        "server:noreload": "zsh scripts/server.sh --no-reload --log-level info --restart",
        "serve": "npm run server",
        "test:server:health": "curl -sSf http://127.0.0.1:8787/healthz | cat",
        "test:ws:smoke": "node chrome-extension/scripts/tests/ws_smoke.mjs",
        "test:extension:logs": "node chrome-extension/scripts/tests/console_assertions.mjs"
    },
    "type": "module",
    "dependencies": {},
    "devDependencies": {
        "ws": "^8.17.0",
        "node-fetch": "^3.3.2"
    }
}
